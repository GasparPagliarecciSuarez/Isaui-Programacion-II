#Filtrar diccionario: Escribe una funci√≥n que reciba un diccionario y una lista de claves,
#y devuelva un nuevo diccionario solo con las claves especificadas.

def Filtrar_Diccionario(diccionario,claves):
    nuevodicc={}
    for clave, valor in diccionario.items():
        if clave in claves:
            nuevodicc[clave]= valor
    return  print (nuevodicc)
    
diccionario = {
        "a":1,
        "b":2,
        "c":3,
        "d":4,
        "e":5,

}
claves = ["c","e","a"]

dicc_filtrado=Filtrar_Diccionario(diccionario,claves)

